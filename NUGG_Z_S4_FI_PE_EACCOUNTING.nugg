  <source>************************************************************************
* Program Name         : Z_S4_FI_IN_EACCOUNTING_SEL                    *
* Created By           : Adan Israel García Reséndiz                   *
* Date                 : Julio 13 2020                                 *
************************************************************************

* Program Description **************************************************
*  Include with define select parameters and events for program        *
*  Z_S4_FI_PE_EACCOUNTING                                              *
************************************************************************

* History Program Changes **********************************************
*                                                                      *
* Transport      Date     Developer          Ver Short Description     *
* ==========  ==========  =================  === ===================== *
* S4DK901325  13-07-2020  Adan García R.     001 Initial Transport     *
************************************************************************

*/* Displays section screen for layout parameter */

SELECTION-SCREEN BEGIN OF BLOCK b02a WITH FRAME TITLE TEXT-003.
  PARAMETERS:
    p_coa   TYPE figlmx_coa        RADIOBUTTON GROUP rb1 USER-COMMAND cmd DEFAULT &apos;X&apos; MODIF ID a01,
    p_bal_n TYPE zfiglmx_balance_n RADIOBUTTON GROUP rb1 MODIF ID  a02,
    p_bal_c TYPE zfiglmx_balance_c RADIOBUTTON GROUP rb1 MODIF ID  a22,
    p_je    TYPE figlmx_je         RADIOBUTTON GROUP rb1 MODIF ID  a03,
    p_aux   TYPE figlmx_aux        RADIOBUTTON GROUP rb1 MODIF ID  a04.

SELECTION-SCREEN END OF BLOCK b02a.

SELECTION-SCREEN BEGIN OF BLOCK b01 WITH FRAME TITLE TEXT-001.
*  SELECT-OPTIONS:
*  s_bukrs FOR l_bukrs   MODIF ID b01.
  PARAMETERS:
  s_bukrs TYPE bkpf-bukrs MODIF ID b01.

  SELECTION-SCREEN BEGIN OF BLOCK c01 WITH FRAME TITLE TEXT-055.

    PARAMETERS:
      p_creppr TYPE figlmx_per    MODIF ID b13,
      p_crepyr TYPE figlmx_year   MODIF ID b12.

  SELECTION-SCREEN END OF BLOCK c01.

  PARAMETERS:
    p_gjahr  TYPE figlmx_year  MODIF ID b03,
    p_rjahr  TYPE bkpf-gjahr   MODIF ID b04,
    p_period TYPE figlmx_per   MODIF ID b05.

  SELECT-OPTIONS: s_budat  FOR l_budat  MODIF ID b09.

  PARAMETERS:
    p_treq TYPE figlmx_req_typ   MODIF ID c03,
    p_ord  TYPE figlmx_ord_no    MODIF ID c04,
    p_pro  TYPE figlmx_pro_no    MODIF ID c05.

  SELECTION-SCREEN SKIP 1.

*
SELECTION-SCREEN END OF BLOCK b01.
*</source>
 </PROG>
 <PROG NAME="Z_S4_FI_IN_EACCOUNTING_DATA" VARCL="X" SUBC="I" APPL="F" RSTAT="K" RMAND="200" RLOAD="S" UCCHECK="X">
  <textPool>
   <language SPRAS="S">
    <textElement ID="R" ENTRY="Include Z_S4_FI_IN_EACCOUNTING_DATA" LENGTH="35 "/>
   </language>
  </textPool>
  <source>************************************************************************
* Program Name         : Z_S4_FI_IN_EACCOUNTING_DATA                   *
* Created By           : Adan Israel García Reséndiz                   *
* Date                 : Julio 13 2020                                 *
************************************************************************

* Program Description **************************************************
*  Include with local declare vars for program Z_S4_FI_IN_EACCOUNTING  *
************************************************************************

* History Program Changes **********************************************
*                                                                      *
* Transport      Date     Developer          Ver Short Description     *
* ==========  ==========  =================  === ===================== *
* S4DK901325  13-07-2020  Adan García R.     001 Initial Transport     *
************************************************************************

*/* Define fileds. */
DATA: l_bukrs       TYPE bkpf-bukrs,
      l_actgrp      TYPE glaccount_screen_coa-ktoks,
      l_budat       TYPE bkpf-budat,
      gv_ngl_active TYPE boole_d.

*/* Define global vars. */
DATA: lt_seltab TYPE TABLE OF rsparams,
      ls_seltab LIKE LINE OF lt_seltab.

DATA: lt_condtab TYPE TABLE OF hrcond,
      ls_condtab LIKE LINE OF lt_condtab.

DATA: wa_zfiteaccounting TYPE zfiteaccounting.

*/* Define clause type. */
TYPES: BEGIN OF ty_s_clause.
TYPES:   line(72)  TYPE c.
TYPES: END OF ty_s_clause.

*/* Define var for where clauses. */
DATA:
  gt_where_clauses  TYPE STANDARD TABLE OF ty_s_clause
                    WITH DEFAULT KEY.

*/* Define var for download file from server SAP. */
CONSTANTS gc_filename TYPE fileintern VALUE &apos;FI_RFASLDPC&apos;.  &quot;n1517472

DATA par_path TYPE afilename.

DATA:                                                       &quot;1776497
  dtastr TYPE TABLE OF string.  &quot;File data for text mode    &quot;1776497
FIELD-SYMBOLS:                                              &quot;1776497
  &lt;fs_line&gt; TYPE string.                                    &quot;1776497

*DATA:
*  hlp_codepage TYPE cpcodepage,                             &quot;989338
*  gui_codepage TYPE abap_encod.                             &quot;989338

DATA:
  hlp_answer(1)   TYPE c,              &quot;Antwort aus Abfrage
  hlp_blz(15)     TYPE c,              &quot;Bankschlüssel
  hlp_file        LIKE rlgrap-filename, &quot;Laufwerk, Verzeichnis, Zielfile
  hlp_file_h      TYPE string,
  hlp_llength     TYPE i,              &quot;Länge gel. Satz     &quot;1776497
  hlp_length      TYPE i,              &quot;Länge des DTA-Files
  hlp_record(128) TYPE c,              &quot;Anfang des Files (zur Analyse)
  hlp_text(132)   TYPE c.              &quot;Ausgabetext

*/* Define screen values. */
DEFINE define_screen_values.

  PERFORM set_value_for_selection_screen
  TABLES
    &amp;1 &quot;General Structure for PARAMETERS and SELECT-OPTIONS
  USING
    &amp;2 &quot;General Structure for PARAMETERS and SELECT-OPTIONS
    &amp;3 &quot;ABAP: Name of SELECT-OPTION/PARAMETER
    &amp;4 &quot;ABAP: Type of selection
    &amp;5 &quot;ABAP: ID: I/E (include/exclude values)
    &amp;6 &quot;ABAP: Selection option (EQ/BT/CP/...)
    &amp;7 &quot;ABAP: Selection Value (LOW or HIGH Value, External Format)
    &amp;8.&quot;ABAP: Selection Value (LOW or HIGH Value, External Format)
END-OF-DEFINITION.

*/* Define screen values. */
DEFINE define_field_value_for_query.

  PERFORM set_field_value_for_query
  TABLES
    &amp;1 &quot;Description of Condition Line
  USING
    &amp;2 &quot;Description of Condition Line
    &amp;3 &quot;Database Field
    &amp;4 &quot;Component of the Version Number
    &amp;5 &quot;LOW Value Field
    &amp;6.&quot;HIGH Value Field
END-OF-DEFINITION.</source>
 </PROG>
 <PROG NAME="Z_S4_FI_IN_EACCOUNTING_FORM" VARCL="X" SUBC="I" APPL="F" RSTAT="K" RMAND="200" RLOAD="S" UCCHECK="X">
  <textPool>
   <language SPRAS="S">
    <textElement ID="R" ENTRY="Include Z_S4_FI_IN_EACCOUNTING_FORM" LENGTH="35 "/>
   </language>
  </textPool>
  <source>************************************************************************
* Program Name         : Z_S4_FI_IN_EACCOUNTING_FORM                   *
* Created By           : Adan Israel García Reséndiz                   *
* Date                 : Julio 13 2020                                 *
************************************************************************

* Program Description **************************************************
*  Include with local forms for program Z_S4_FI_IN_EACCOUNTING         *
************************************************************************

* History Program Changes **********************************************
*                                                                      *
* Transport      Date     Developer          Ver Short Description     *
* ==========  ==========  =================  === ===================== *
* S4DK901325  13-07-2020  Adan García R.     001 Initial Transport     *
************************************************************************


*:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
*   FORM SCREEN_INVISIBLE                                              :
*:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
*      Form that controls which fields are shown or not according to   :
*      the type of file                                                :
*:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
FORM screen_invisible .

  CALL FUNCTION &apos;FAGL_CHECK_GLFLEX_ACTIVE&apos;
    IMPORTING
      e_glflex_active = gv_ngl_active
    EXCEPTIONS
      error_in_setup  = 1
      OTHERS          = 2.

  IF sy-subrc &lt;&gt; 0.
*      MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno
*          WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4.
  ENDIF.

  IF p_coa IS NOT INITIAL OR ( p_bal_n IS NOT INITIAL ).
    LOOP AT SCREEN.
      IF screen-name = &apos;P_RJAHR&apos; OR screen-group1 = &apos;B04&apos;.
        screen-active = &apos;0&apos;.
      ENDIF.
      IF screen-name = &apos;P_TREQ&apos;  OR screen-group1 = &apos;C03&apos;.
        screen-active = &apos;0&apos;.
      ENDIF.
      IF screen-name = &apos;P_ORD&apos;   OR screen-group1 = &apos;C04&apos;.
        screen-active = &apos;0&apos;.
      ENDIF.
      IF screen-name = &apos;P_PRO&apos;   OR screen-group1 = &apos;C05&apos;.
        screen-active = &apos;0&apos;.
      ENDIF.
      IF screen-name = &apos;P_CSVOPT&apos;   OR screen-group1 = &apos;G03&apos;.
        screen-active = &apos;0&apos;.
      ENDIF.
      IF screen-name = &apos;CSV_PATH&apos;   OR screen-group1 = &apos;G04&apos;.
        screen-active = &apos;0&apos;.
      ENDIF.
      IF screen-name = &apos;S_UMSKZ-LOW&apos;  OR screen-name = &apos;S_UMSKZ-HIGH&apos;  OR ( screen-group1 = &apos;C06&apos; ) OR
        ( screen-group3 = &apos;VPU&apos; AND screen-group1 = &apos;C06&apos;  ).
        screen-active = &apos;0&apos;.
      ENDIF.
      IF screen-name = &apos;S_BUDAT-LOW&apos; OR screen-name = &apos;S_BUDAT-HIGH&apos; OR ( screen-group1 = &apos;B09&apos;) OR
        ( screen-group3 = &apos;VPU&apos; AND screen-group1 = &apos;B09&apos;  ).
        screen-active = &apos;0&apos;.
      ENDIF.
      IF screen-name = &apos;S_SAKNR-LOW&apos; OR screen-name = &apos;S_SAKNR-HIGH&apos; OR ( screen-group1 = &apos;E01&apos; ) OR
        ( screen-group3 = &apos;VPU&apos; AND screen-group1 = &apos;E01&apos;  ).
        screen-active = &apos;0&apos;.
      ENDIF.
      IF screen-name = &apos;S_TAXID-LOW&apos; OR screen-name = &apos;S_TAXID-HIGH&apos; OR ( screen-group1 = &apos;E02&apos; ) OR
        ( screen-group3 = &apos;VPU&apos; AND screen-group1 = &apos;E02&apos;  ).
        screen-active = &apos;0&apos;.
      ENDIF.
      IF screen-name = &apos;S_UUID-LOW&apos;  OR screen-name = &apos;S_UUID-HIGH&apos;  OR ( screen-group1 = &apos;E03&apos; ) OR
        ( screen-group3 = &apos;VPU&apos; AND screen-group1 = &apos;E03&apos;  ).
        screen-active = &apos;0&apos;.
      ENDIF.

      IF screen-name = &apos;S_BELNR-LOW&apos;  OR screen-name = &apos;S_BELNR-HIGH&apos;  OR ( screen-group1 = &apos;E04&apos; ) OR
        ( screen-group3 = &apos;VPU&apos; AND screen-group1 = &apos;E04&apos;  ).
        screen-active = &apos;0&apos;.
      ENDIF.

      IF screen-name = &apos;S_BLART-LOW&apos;  OR screen-name = &apos;S_BLART-HIGH&apos;  OR ( screen-group1 = &apos;E05&apos; ) OR
        ( screen-group3 = &apos;VPU&apos; AND screen-group1 = &apos;E05&apos;  ).
        screen-active = &apos;0&apos;.
      ENDIF.

      IF screen-name = &apos;P_ALVOPT&apos;      OR screen-group1 = &apos;G02&apos;        OR
        ( screen-group3 = &apos;VPU&apos; AND screen-group1 = &apos;G02&apos;  ).
        screen-active = &apos;0&apos;.
      ENDIF.
      MODIFY SCREEN.
    ENDLOOP.
  ELSEIF p_je IS NOT INITIAL.
    LOOP AT SCREEN.
      IF screen-name = &apos;P_GJAHR&apos;  OR ( screen-group1 = &apos;B03&apos;  ).
        screen-active = &apos;0&apos;.
      ENDIF.
      IF screen-name = &apos;P_PERIOD&apos; OR ( screen-group1 = &apos;B05&apos;  ).
        screen-active = &apos;0&apos;.
      ENDIF.
      IF screen-name = &apos;P_FSV&apos;    OR ( screen-group1 = &apos;B06&apos;  ).
        screen-active = &apos;0&apos;.
      ENDIF.
      IF gv_ngl_active = space.
        IF screen-name = &apos;P_LEDGER&apos; OR ( screen-group1 = &apos;B07&apos;  ).
          screen-active = &apos;0&apos;.
        ENDIF.
      ENDIF.
      IF screen-name = &apos;S_ACTGRP-LOW&apos; OR screen-name = &apos;S_ACTGRP-HIGH&apos; OR screen-group1 = &apos;B08&apos;  OR
        ( screen-group3 = &apos;VPU&apos; AND screen-group1 = &apos;B08&apos;  ).
        screen-active = &apos;0&apos;.
      ENDIF.
      IF screen-name = &apos;P_FTYP&apos;       OR screen-name = &apos;P_MOD&apos;         OR screen-group1 = &apos;C01&apos;  OR
         screen-group1 = &apos;C02&apos; .
        screen-active = &apos;0&apos;.
      ENDIF.
      MODIFY SCREEN.
    ENDLOOP.
  ENDIF.

  IF p_coa IS NOT INITIAL OR gv_ngl_active = space.
    LOOP AT SCREEN.
      IF screen-name = &apos;P_LEDGER&apos; OR ( screen-group1 = &apos;B07&apos;  ).
        screen-active = &apos;0&apos;.
        MODIFY SCREEN.
      ENDIF.
      IF p_coa IS NOT INITIAL.
        IF screen-name = &apos;P_FTYP&apos; OR screen-name = &apos;P_MOD&apos; OR
          ( screen-group1 = &apos;C01&apos; OR screen-group1 = &apos;C02&apos; ).
          screen-active = &apos;0&apos;.
          MODIFY SCREEN.
        ENDIF.
      ENDIF.
    ENDLOOP.
  ENDIF.

  IF p_aux IS NOT INITIAL.

    LOOP AT SCREEN.
      IF screen-name = &apos;P_RJAHR&apos; OR screen-group1 = &apos;B04&apos;.
        screen-active = &apos;0&apos;.
        MODIFY SCREEN.
      ENDIF.
      IF screen-name = &apos;S_BUDAT-LOW&apos; OR screen-name = &apos;S_BUDAT-HIGH&apos; OR screen-group1 = &apos;B09&apos; OR
        ( screen-group3 = &apos;VPU&apos; AND screen-group1 = &apos;B09&apos; ).
        screen-active = &apos;0&apos;.
        MODIFY SCREEN.
      ENDIF.
      IF screen-name = &apos;P_FTYP&apos; OR screen-group1 = &apos;C01&apos;.
        screen-active = &apos;0&apos;.
        MODIFY SCREEN.
      ENDIF.
      IF screen-name = &apos;P_MOD&apos; OR screen-group1 = &apos;C02&apos;.
        screen-active = &apos;0&apos;.
        MODIFY SCREEN.
      ENDIF.
      IF screen-name = &apos;P_CSVOPT&apos;   OR screen-group1 = &apos;G03&apos;.
        screen-active = &apos;0&apos;.
        MODIFY SCREEN.
      ENDIF.
      IF screen-name = &apos;CSV_PATH&apos;   OR screen-group1 = &apos;G04&apos;.
        screen-active = &apos;0&apos;.
        MODIFY SCREEN.
      ENDIF.
      IF screen-name = &apos;S_UMSKZ-LOW&apos;  OR screen-name = &apos;S_UMSKZ-HIGH&apos;  OR ( screen-group1 = &apos;C06&apos; ) OR
        ( screen-group3 = &apos;VPU&apos; AND screen-group1 = &apos;C06&apos;  ).
        screen-active = &apos;0&apos;.
      ENDIF.
      IF screen-name = &apos;S_TAXID-LOW&apos; OR screen-name = &apos;S_TAXID-HIGH&apos; OR screen-group1 = &apos;E02&apos; OR
        ( screen-group3 = &apos;VPU&apos; AND screen-group1 = &apos;E02&apos; ).
        screen-active = &apos;0&apos;.
        MODIFY SCREEN.
      ENDIF.
      IF screen-name = &apos;S_UUID-LOW&apos; OR screen-name = &apos;S_UUID-HIGH&apos; OR screen-group1 = &apos;E03&apos; OR
        ( screen-group3 = &apos;VPU&apos; AND screen-group1 = &apos;E03&apos; ).
        screen-active = &apos;0&apos;.
        MODIFY SCREEN.
      ENDIF.

      IF screen-name = &apos;S_BELNR-LOW&apos;  OR screen-name = &apos;S_BELNR-HIGH&apos;  OR ( screen-group1 = &apos;E04&apos; ) OR
        ( screen-group3 = &apos;VPU&apos; AND screen-group1 = &apos;E04&apos;  ).
        screen-active = &apos;0&apos;.
        MODIFY SCREEN.
      ENDIF.

      IF screen-name = &apos;S_BLART-LOW&apos;  OR screen-name = &apos;S_BLART-HIGH&apos;  OR ( screen-group1 = &apos;E05&apos; ) OR
        ( screen-group3 = &apos;VPU&apos; AND screen-group1 = &apos;E05&apos;  ).
        screen-active = &apos;0&apos;.
        MODIFY SCREEN.
      ENDIF.

    ENDLOOP.
  ENDIF.

  IF p_aux IS INITIAL.
    LOOP AT SCREEN.
      IF screen-group1 = &apos;G05&apos;.
        screen-active = &apos;0&apos;.
        MODIFY SCREEN.
      ENDIF.
    ENDLOOP.
  ENDIF.

  IF ( p_bal_n IS INITIAL ).

    LOOP AT SCREEN.

      IF screen-group1 EQ &apos;B10&apos; OR screen-group1 EQ &apos;B11&apos; OR screen-group1 EQ &apos;B12&apos;
      OR screen-group1 EQ &apos;B13&apos; OR screen-group1 EQ &apos;B14&apos; OR screen-group1 EQ &apos;B15&apos;
      OR screen-group1 EQ &apos;B16&apos; OR screen-group1 EQ &apos;B17&apos; OR screen-group1 EQ &apos;B18&apos;
      OR screen-group1 EQ &apos;B19&apos; OR screen-group1 EQ &apos;B20&apos; OR screen-group1 EQ &apos;B21&apos;
      OR screen-group1 EQ &apos;B22&apos; .
        screen-active = &apos;0&apos;.
        MODIFY SCREEN.
      ENDIF.

    ENDLOOP.

  ELSEIF ( p_bal_n IS NOT INITIAL ).

    LOOP AT SCREEN.

      IF screen-group1 EQ &apos;B03&apos; OR screen-group1 EQ &apos;B05&apos;.
        screen-active = &apos;0&apos;.
        MODIFY SCREEN.
      ENDIF.
    ENDLOOP.
  ENDIF.

ENDFORM.
