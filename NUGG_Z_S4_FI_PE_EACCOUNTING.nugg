  obj_ls_condtab-high = high.

*/* Add structure to internal table. */
  APPEND obj_ls_condtab TO obj_lt_condtab.

*/* Clean structure. */
  CLEAR obj_ls_condtab.
ENDFORM.                    &quot;SET_FIELD_VALUE_FOR_QUERY
