*::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
*       FORM WRITE_TEXT                                               *
*::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
*       ........                                                      *
*::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
*  --&gt;  TEXT                                                          *
*::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
FORM write_text USING text.

  hlp_text = text.
  REPLACE:
    &apos;&amp;1&apos; WITH hlp_file INTO hlp_text,
    &apos;&amp;2&apos; WITH space INTO hlp_text,
    &apos;&amp;3&apos; WITH par_path INTO hlp_text.
  CONDENSE hlp_text.
  WRITE / hlp_text.

ENDFORM.
